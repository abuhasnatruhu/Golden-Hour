# API Configuration
# Copy this file to .env and fill in your actual API keys

# Unsplash API (for photography inspiration images)
NEXT_PUBLIC_UNSPLASH_ACCESS_KEY=your_unsplash_access_key_here
UNSPLASH_SECRET_KEY=your_unsplash_secret_key_here

# Pexels API (for location-based imagery)
NEXT_PUBLIC_PEXELS_API_KEY=your_pexels_api_key_here

# OpenWeatherMap API (for weather data)
NEXT_PUBLIC_OPENWEATHER_API_KEY=your_openweather_api_key_here

# Application Configuration
NEXT_PUBLIC_APP_URL=http://localhost:3000
NODE_ENV=development

# Database (optional - uses local SQLite by default)
DATABASE_URL="file:./prisma/db/custom.db"